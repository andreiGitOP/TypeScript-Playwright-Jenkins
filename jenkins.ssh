pipeline {
    agent any

    tools{nodejs "recent ode"}
    
    stages {               
        stage('Install node') {
            steps {               
                    sh 'npm install -D @playwright/test'
                }
            }
        
        
        stage('Run Playwright Tests') {
            steps {                
                nodejs('recent node') {
                    sh 'npm run testCase'
                }
            }
        }
    }
}